{
  "git": {
    "commitMessage": "chore: release v${version}",
    "requireBranch": "main",
    "tag": false,
    "push": true
  },
  "github": { "release": false },
  "npm": { "publish": false },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "addUnreleased": true,
      "addVersionUrl": true
    }
  }
}
